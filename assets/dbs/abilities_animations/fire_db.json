{
    "key_name": "fire",
    "cast_type": "psynergy_cast",
    "wait_for_cast_animation": false,
    "sprites": [{
        "key_name": "cast",
        "per_target": true,
        "position": "between"
    }],
    "stage_angle_sequence": [{
      "is_absolute": false,
      "tween": "Quadratic.Out",
      "duration": 500,
      "start_delay": 500,
      "direction": "closest",
      "to": 0
    }],
    "particles_sequence": [{
        "data": {
            "fire_balls" : {
                "red": 255,
                "green": 10,
                "blue": 0,
                "lifespan": 5000,
                "transform_control": [
                    {"x": 0, "transforms": ["particles_out"]},
                    {"x": 0.5, "transforms": ["particles_in"], "copy": [{"from": "particles_out", "to": "particles_in"}]}
                ],
                "velocity": {
                    "transform_key": "particles_out",
                    "initial": {"min": 0.8, "max": 2.5},
                    "radial": { "arcStart": -95, "arcEnd": 95 },
                    "control": [ { "x": 0, "y": 1 }, { "x": 0.5, "y": 0 } ]
                },
                "target": {
                    "transform_key": "particles_in",
                    "x": "targets",
                    "y": "targets",
                    "duration": 150,
                    "control": [
                        { "x": 0, "y": 0 },
                        { "x": 0.5, "y": 0, "refresh_target": true },
                        { "x": 1, "y": 1 }
                    ]
                }
            }
        },
        "zones": {},
        "emitters": [{
            "x": "caster",
            "y": "caster",
            "transforms": ["particles_out", "particles_in"],
            "shift_y": -40,
            "position": "between",
            "render_type": "pixel",
            "show_trails": true,
            "trails_clear_factor": 0.3,
            "pixel_size": 6,
            "pixel_is_rect": false,
            "particles_display_blend_mode": "screen",
            "render_white_core": true,
            "core_custom_color": "rgba(255,255,255,0.85)",
            "total": 250,
            "frequency": 10,
            "emitter_data_key": "fire_balls",
            "delay": {
                "start": 1000,
                "visible": false
            }
        }],
        "emission_finish": 5000
    }]
}